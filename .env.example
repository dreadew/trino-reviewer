# Основные настройки приложения
APP_NAME="SQL RecSys"
LOG_LEVEL="INFO"

# Настройки LLM (выберите один из типов: giga, openai, gemini)
MODEL_TYPE="giga"
API_KEY="your-api-key-here"
MAX_TOKENS=2048
TEMPERATURE=0.1

# GigaChat settings (если MODEL_TYPE=giga)
MODEL_NAME="GigaChat"

# OpenAI settings (если MODEL_TYPE=openai)
OPENAI_API_KEY="your-openai-api-key"
OPENAI_MODEL_NAME="gpt-4o-mini"
# OPENAI_BASE_URL="https://api.openai.com/v1"  # Опционально для кастомных эндпоинтов

# Google Gemini settings (если MODEL_TYPE=gemini)
GOOGLE_API_KEY="your-google-api-key"
GEMINI_MODEL_NAME="gemini-1.5-flash"

# Настройки gRPC сервера
GRPC_PORT=50051
GRPC_MAX_WORKERS=10
GRPC_GRACE_PERIOD=5

# Настройки чата
MAX_CHAT_HISTORY_SIZE=10

# Workflow настройки
DEFAULT_THREAD_ID="default"

# База данных (опционально)
# ASYNC_DSN="postgresql+asyncpg://user:password@localhost/dbname"
# CLEAN_DSN="postgresql://user:password@localhost/dbname"